{
  "snippets.extends": {
    "vue": ["javascript"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"],
    "typescriptreact": ["typescript"]
  },
  "snippets.ultisnips.enable": true,
  "languageserver": {
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile", "Dockerfile"],
      "args": ["--stdio"]
    }
  },
  "diagnostic.enableSign": true,
  "diagnostic.virtualText": false,
  "diagnostic.errorSign": "✕",
  "diagnostic.warningSign": "△",
  "diagnostic.infoSign": "⌽",
  "diagnostic.hintSign": "⚡︎",
  "css.colorDecorators.enable": false,
  "coc.preferences.colorSupport": false,
  "coc.preferences.formatOnType": false,
  "coc.preferences.formatOnSaveFiletypes": ["typescript", "typescriptreact"],
  "vetur.format.enable": true,
  "vetur.completion.autoImport": true,
  "vetur.validation.style": true,
  "vetur.validation.script": true,
  "vetur.validation.template": true,
  "vetur.format.defaultFormatter.js": "prettier",
  "vetur.format.defaultFormatter.ts": "prettier",
  "vetur.format.defaultFormatter.css": "prettier",
  "vetur.format.defaultFormatter.html": "prettier",
  "vetur.experimental.templateInterpolationService": false,
  "eslint.enable": true,
  "eslint.run": "onType",
  "eslint.autoFix": true,
  "eslint.autoFixOnSave": true,
  "eslint.packageManager": "npm",
  "eslint.filetypes": ["javascript", "javascript.jsx", "vue"],
  "prettier.eslintIntegration": false,
  "prettier.stylelintIntegration": false,
  "prettier.formatterPriority": 1,
  "prettier.disableLanguages": ["vue"],
  "jest.watchman": true,
  "tsserver.enable": true,
  "typescript.updateImportsOnFileMove.enable": true,
  "typescript.suggest.autoImports": true,
  "typescript.referencesCodeLens.enable": true,
  "typescript.implementationsCodeLens.enable": true,
  "typescript.suggestionActions.enabled": true,
  "typescript.showUnused": true,
  "typescript.suggest.paths": true,
  "typescript.format.enabled": true,
  "typescript.suggest.enabled": true,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.preferences.quoteStyle": "auto",
  "tsserver.disableAutomaticTypeAcquisition": false
}
