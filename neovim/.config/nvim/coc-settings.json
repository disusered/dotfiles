{
  "snippets.extends": {
    "javascriptreact": [
      "javascript"
    ],
    "typescript": [
      "javascript"
    ]
  },
  "languageserver": {
    "vimls": {
      "command": "vim-language-server",
      "args": ["--stdio"],
      "initializationOptions": {
        "iskeyword": "vim iskeyword option",
        "vimruntime": "path/to/$VIMRUNTIME",
        "runtimepath": "vim/runtime/path",
        "diagnostic": {
          "enable": true
        }
      },
      "filetypes": ["vim"]
    },
    "elixirLS": {
      "command": "/Users/carlos/.config/coc/elixir-ls/build/language_server.sh",
      "filetypes": [
        "elixir",
        "eelixir"
      ]
    },
    "golang": {
      "command": "gopls",
      "args": [],
      "rootPatterns": [
        "go.mod",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "filetypes": [
        "go"
      ]
    },
    "bash": {
      "command": "bash-language-server",
      "args": [
        "start"
      ],
      "filetypes": [
        "sh"
      ],
      "ignoredRootPaths": [
        "~"
      ]
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": [
        "dockerfile",
        "Dockerfile"
      ],
      "args": [
        "--stdio"
      ]
    },
    "flow": {
      "command": "flow",
      "args": [
        "lsp"
      ],
      "filetypes": [
        "javascript",
        "javascriptreact"
      ],
      "initializationOptions": {},
      "requireRootPattern": true,
      "settings": {},
      "rootPatterns": [
        ".flowconfig"
      ]
    }
  },
  "diagnostic.enableSign": true,
  "diagnostic.virtualText": false,
  "diagnostic.locationlist": true,
  "diagnostic.errorSign": "✕",
  "diagnostic.warningSign": "△",
  "diagnostic.infoSign": "⌽",
  "diagnostic.hintSign": "⚡︎",
  "css.colorDecorators.enable": false,
  "coc.preferences.colorSupport": false,
  "eslint.enable": true,
  "eslint.run": "onType",
  "eslint.autoFix": true,
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": [
    "javascript",
    "javascript.jsx",
    "vue"
  ],
  "prettier.eslintIntegration": true,
  "prettier.stylelintIntegration": true,
  "prettier.formatterPriority": 1,
  "jest.watchman": true
}
