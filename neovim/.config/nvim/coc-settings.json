{
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "languageserver": {
    "elixirLS": {
      "command": "/Users/carlos/.config/coc/elixir-ls/build/language_server.sh",
      "filetypes": ["elixir", "eelixir"]
    },
    "golang": {
      "command": "gopls",
      "args": [],
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    },
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile", "Dockerfile"],
      "args": ["--stdio"]
    }
  },
  "diagnostic.enableSign": true,
  "diagnostic.virtualText": false,
  "diagnostic.locationlist": true,
  "diagnostic.errorSign": "✕",
  "diagnostic.warningSign": "△",
  "diagnostic.infoSign": "⌽",
  "diagnostic.hintSign": "⚡︎",
  "css.colorDecorators.enable": false,
  "coc.preferences.colorSupport": false,
  "eslint.enable": true,
  "eslint.run": "onType",
  "eslint.autoFix": true,
  "eslint.autoFixOnSave": true,
  "eslint.filetypes": ["javascript", "javascript.jsx", "vue"],
  "prettier.eslintIntegration": true,
  "prettier.stylelintIntegration": true,
  "prettier.formatterPriority": -1
}
