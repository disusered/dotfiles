{
  "snippets.extends": {
    "vue": ["javascript"],
    "javascriptreact": ["javascript"],
    "typescript": ["javascript"]
  },
  "languageserver": {
    "dls": {
      "command": "diagnostic-languageserver",
      "args": ["--stdio"],
      "filetypes": ["markdown", "sh", "vim"],
      "initializationOptions": {
        "linters": {
          "languagetool": {
            "command": "languagetool",
            "debounce": 200,
            "args": ["-"],
            "offsetLine": 0,
            "offsetColumn": 0,
            "sourceName": "languagetool",
            "formatLines": 2,
            "formatPattern": [
              "^\\d+?\\.\\)\\s+Line\\s+(\\d+),\\s+column\\s+(\\d+),\\s+([^\\n]+)\nMessage:\\s+(.*)$",
              {
                "line": 1,
                "column": 2,
                "message": [4, 3]
              }
            ]
          },
          "shellcheck": {
            "command": "shellcheck",
            "debounce": 100,
            "args": ["--format=gcc", "-"],
            "offsetLine": 0,
            "offsetColumn": 0,
            "sourceName": "shellcheck",
            "formatLines": 1,
            "formatPattern": [
              "^[^:]+:(\\d+):(\\d+):\\s+([^:]+):\\s+(.*)$",
              {
                "line": 1,
                "column": 2,
                "message": 4,
                "security": 3
              }
            ],
            "securities": {
              "error": "error",
              "warning": "warning",
              "note": "info"
            }
          },
          "vint": {
            "command": "vint",
            "debounce": 100,
            "args": ["--enable-neovim", "-"],
            "offsetLine": 0,
            "offsetColumn": 0,
            "sourceName": "vint",
            "formatLines": 1,
            "formatPattern": [
              "[^:]+:(\\d+):(\\d+):\\s*(.*$)",
              {
                "line": 1,
                "column": 2,
                "message": 3
              }
            ]
          }
        },
        "filetypes": {
          "markdown": "languagetool",
          "sh": "shellcheck",
          "vim": "vint"
        },
        "formatters": {},
        "formatFiletypes": {}
      }
    },
    "vimls": {
      "command": "vim-language-server",
      "args": ["--stdio"],
      "initializationOptions": {
        "iskeyword": "vim iskeyword option",
        "vimruntime": "path/to/$VIMRUNTIME",
        "runtimepath": "vim/runtime/path",
        "diagnostic": {
          "enable": true
        }
      },
      "filetypes": ["vim"]
    },
    "elixirLS": {
      "command": "/Users/carlos/.config/coc/elixir-ls/build/language_server.sh",
      "filetypes": ["elixir", "eelixir"]
    },
    "golang": {
      "command": "gopls",
      "args": [],
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"]
    },
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh"],
      "ignoredRootPaths": ["~"]
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile", "Dockerfile"],
      "args": ["--stdio"]
    }
  },
  "diagnostic.enableSign": true,
  "diagnostic.virtualText": false,
  "diagnostic.locationlist": true,
  "diagnostic.errorSign": "✕",
  "diagnostic.warningSign": "△",
  "diagnostic.infoSign": "⌽",
  "diagnostic.hintSign": "⚡︎",
  "css.colorDecorators.enable": false,
  "coc.preferences.colorSupport": false,
  "vetur.format.enable": true,
  "vetur.completion.autoImport": true,
  "vetur.validation.style": true,
  "vetur.validation.script": true,
  "vetur.validation.template": true,
  "vetur.format.defaultFormatter.js": "prettier",
  "vetur.format.defaultFormatter.ts": "prettier",
  "vetur.format.defaultFormatter.css": "prettier",
  "vetur.format.defaultFormatter.html": "prettier",
  "vetur.experimental.templateInterpolationService": false,
  "eslint.enable": true,
  "eslint.run": "onType",
  "eslint.autoFix": true,
  "eslint.autoFixOnSave": true,
  "eslint.packageManager": "npm",
  "eslint.filetypes": ["javascript", "javascript.jsx", "vue"],
  "prettier.eslintIntegration": false,
  "prettier.stylelintIntegration": false,
  "prettier.formatterPriority": 1,
  "prettier.disableLanguages": ["vue"],
  "jest.watchman": true,
  "tsserver.enable": true,
  "tsserver.npm": "/Users/carlos/.nvm/versions/node/v11.1.0/bin/npm" 
}
